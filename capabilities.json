          "category": { "max": 1 },
          "measure": { "max": 1 },
          "details": { "max": 0 }
        }
      ],
      "categorical": {
        "categories": {
          "for": { "in": "category" },
          "dataReductionAlgorithm": { "top": { "count": 5000 } }
        },
        "values": {
          "select": [
            { "bind": { "to": "measure" } }
          ]
        }
      }
    },
    {
      "conditions": [
        {
          "category": { "max": 1 },
          "measure": { "max": 1 },
          "details": { "min": 1, "max": 1 }
        }
      ],
      "categorical": {
        "categories": {
          "select": [
            { "for": { "in": "category" } },
            { "for": { "in": "details" } }
          ],
          "dataReductionAlgorithm": { "top": { "count": 500000 } }
        },
        "values": {
          "select": [
            { "bind": { "to": "measure" } }
          ]
        }
      }
    }
